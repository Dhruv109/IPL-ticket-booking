<meta content="width=device-width, initial-scale=1" name="viewport" />
<link
  href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
  rel="stylesheet"
  integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
  crossorigin="anonymous"
/>
<link type="text/css" rel="stylesheet" href="/cssforejs.css" />
<link type="text/css" rel="stylesheet" href="/css/mybooking.css" />
<body>
  <div class="container">
    <div class="head">
      <h1>My Bookings</h1>
      <a href="/rl/dashboard"><button>Home</button></a>
    </div>
    <div class="card-container">
      <% bookings.forEach((ticket)=>{ %>
      <div class="card">
        <h3 class="versus"><%=ticket.team_1%> vs <%=ticket.team_2%></h3>
        <div class="left">
          <h5>City : <%=ticket.city%></h5>
          <h5>Date : <%=ticket.date%></h5>
          <h5>Time : <%=ticket.time%></h5>
        </div>
        <div class="right">
          <h5>Seat Category : <%=ticket.category%></h5>
          <div class="seats">
            <h5>Seat Number(s):</h5>
            <%ticket.seats.forEach((seat)=>{%>
            <h5 class="seat"><%=seat%></h5>
            <% }); %>
          </div>
        </div>
      </div>
      <% }); %>
    </div>
  </div>
</body>
